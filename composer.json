{
  "name": "mihkullorg/lhv-connect",
  "description": "LHV bank's CONNECT service for Laravel 5",
  "keywords": ["lhv","lhv-connect", "connect", "lhv-bank", "bank"],
  "homepage": "https://github.com/mihkullorg/lhv-connect",
  "license": "MIT",
  "authors": [
    {
      "name": "Mihkel Allorg",
      "email": "mihkel.allorg@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/config": "*",
    "illuminate/support": "*",
    "guzzlehttp/guzzle": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Mihkullorg\\LhvConnect\\": "src/Mihkullorg/LhvConnect"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mihkullorg\\LhvConnect\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
        "providers": [
            "Mihkullorg\\LhvConnect\\LaravelLhvConnectServiceProvider"
        ]
    }
  }
}
